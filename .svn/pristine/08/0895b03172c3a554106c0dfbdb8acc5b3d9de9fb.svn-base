<view class="warp">
<view class="section">
    <text class="title">报修单号:</text>
    <text>{{schoolname}}</text>
  </view>
  <view class="section">
    <text class="title">所属学校:</text>
    <text>{{schoolname}}</text>
  </view>
    <view class="section">
    <text class="title">设备型号:</text>
  
  </view>
  <view class="section">
    <text class="title">设备类型:</text>
  
  </view>
  <view class="section">
    <text class="title">设备标识:</text>
    <text>{{33}}</text>
  </view>
  
  <view class="section">
    <text class="title">安装位置:</text>
    <text>{{33}}</text>
  </view>
  <view class="section">
    <text class="title">连接方式:</text>
    <text>{{33}}</text>
  </view>
  <view class="blodline"></view>
  <view class="section">
    <text class="title">维修人员:</text>
    <text>{{33}}</text>
  </view>
  <view class="section">
    <text class="title">报修人:</text>
    <picker bindchange="bindPickerChange" value="{{typeIndex}}" range-key="{{'name'}}" range="{{objectArray}}">
      {{objectArray[typeIndex].name}}
    </picker>
  </view>
  <view class="blodline"></view>
  <view class="remarkWarp">
    <text class="title">备注:</text>
    <textarea class="remark"></textarea>
  </view>
  <add-pic bind:getPicList="getPicList"></add-pic>
</view>

<view class="submitAdd">申请报修</view>