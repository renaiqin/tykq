/* pages/signIn/linkMan/linkMan.wxss */

.linkLabel{
  width: 100%;
  height: 100rpx;
  display: block;
  border-bottom:1rpx solid #F7F7F7;
}
.radioWarp{
  width: 90%;
  margin:20rpx auto;
}
.linkImg{
  width: 70rpx;
  height: 70rpx;
  margin-top:20rpx;
  
}

.linkRideo{
  width: 70rpx;
  height: 70rpx;
  margin-top:20rpx;
}

.linkText{
  line-height: 100rpx;
  display:block;
  margin-left: 20rpx; 
}

.finish{
  width: 90%;
  height: 80rpx;
  position: fixed;
  left: 5%;
  font-size: 30rpx;
  bottom:40rpx;
  text-align: center;
  line-height: 80rpx;
  color:#fff;
  background-color: #4787F1;
  border-radius: 20rpx;
}

.searchWarp{
  width: 100%;
  height: 90rpx;
  position: relative;
  overflow: hidden;
}
.searchInput{
  width: 80%;
  margin:0rpx auto;
  height: 70rpx;
  background-color: #fff;
  border:1rpx solid #E3E3E3;
  font-size: 30rpx;
  padding-left: 70rpx;
}
.searchIcon{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 15rpx;
  left: 60rpx;
}