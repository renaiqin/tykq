.tab{
  width: 420rpx;
  height: 60rpx;
  margin:0 auto;
}
.normalTab{
  width: 198rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  text-align: center;
  border:1rpx solid #4C84F1;
}

.tabLeft{
  border-top-left-radius: 26rpx;
  border-bottom-left-radius: 26rpx;
}
.tabRight{
  border-top-right-radius: 26rpx;
  border-bottom-right-radius: 26rpx;
}

.activeTab{
  background-color: #4C84F1;
  color:#fff;
}
.item{
  width: 150rpx;
  height: 190rpx;
  display: block;
  margin-left: 15rpx;
  margin-top:30rpx;
}
.item-image{
  width: 150rpx;
  height: 150rpx;
  display: block;
  margin-left: 0rpx;
}
.item-text{
  display: block;
  text-align: center;
}
.item-warp{
  width: 90%;
  margin: 0 auto;
}
.tips{
  width: 300rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  position: fixed;
  bottom:50rpx;
  left: 230rpx;
  font-size: 30rpx;
  background-color: #4C84F1;
  color:#fff;
  border-radius: 40rpx;
}